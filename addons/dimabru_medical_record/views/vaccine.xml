<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        
        <record id="vaccine_tree_view" model="ir.ui.view">
            <field name="name">vaccine.tree</field>
            <field name="model">vaccine</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="description"/>

                </tree>
            </field>
        </record>

       
        <record id="vaccine_form_view" model="ir.ui.view">
            <field name="name">vaccine.form</field>
            <field name="model">vaccine</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="description"/>
       
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="vaccine_action" model="ir.actions.act_window">
            <field name="name">Vaccine</field>
            <field name="res_model">vaccine</field>
            <field name="view_mode">tree,form</field>
        </record>

        
    </data>
</odoo>
